{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug with MSYS2",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/emc.exe",
      "args": [
        "program.em",
        "-asm"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build-msys"
    }
  ]
}
